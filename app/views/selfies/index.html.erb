<h1>Listing selfies</h1>

<table>
  <thead>
    <tr>
      <th>Image url</th>
      <th>Json analysis</th>
      <th>Votes</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @selfies.each do |selfy| %>
      <tr>
        <td><%= selfy.image_url %></td>
        <td><%= selfy.json_analysis %></td>
        <td><%= selfy.votes %></td>
        <td><%= selfy.latitude %></td>
        <td><%= selfy.longitude %></td>
        <td><%= link_to 'Show', selfy %></td>
        <td><%= link_to 'Edit', edit_selfy_path(selfy) %></td>
        <td><%= link_to 'Destroy', selfy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Selfy', new_selfy_path %>
